local function getTarget()
    local char = LocalPlayer.Character
    if not char or not char:FindFirstChild("HumanoidRootPart") then return nil end
    local target, dist = nil, Config.Radius
    for _, obj in pairs(Workspace:GetDescendants()) do
        if obj:IsA("Model") and obj ~= char then
            local hum = obj:FindFirstChildOfClass("Humanoid")
            local hrp = obj:FindFirstChild("HumanoidRootPart")
            if hum and hum.Health > 0 and hrp then
                if Config.IgnoreFriends then
                    local p = Players:GetPlayerFromCharacter(obj)
                    if p and LocalPlayer:IsFriendsWith(p.UserId) then continue end
                end
                local d = (char.HumanoidRootPart.Position - hrp.Position).Magnitude
                if d < dist then dist = d; target = obj end
            end
        end
    end
    return target
end

local function fireOneShot()
    local target = getTarget()
    local emoteObj = emotesFolder:FindFirstChild(Config.OneShotEmote)
    if target and emoteObj then
        _G.KillEmote = true
        run_as_identity_2(function()
            pcall(Core.Get("Combat", "Ability").Activate, emoteObj, target)
        end)
        _G.KillEmote = false
    end
end

