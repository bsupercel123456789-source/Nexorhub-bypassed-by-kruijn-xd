local function initializeHitbox()
    if typeof(core) == "table" and not originalHitBox then
        local ok, combat = pcall(core.Get, "Combat", "Hit")
        if ok and combat and combat.Box then
            originalHitBox = combat.Box
        end
    end
end

-- === ÐÐÐÐÐÐ Ð¥ÐÐ¢ÐÐÐÐ¡Ð ===
local function updateHitboxState(enabled)
    if not core then return end
    local ok, combat = pcall(core.Get, "Combat", "Hit")
    if not ok or not combat then return end

    if not originalHitBox then initializeHitbox() end

    if enabled then
        combat.Box = function(...)
            local args = {...}
            if deathCheck or not getgenv().HitboxEnabled or not args[3] or type(args[3]) ~= "table" then
                return originalHitBox(...)
            end

            -- Ð Ð°ÑÑÐµÑ ÑÐ°Ð·Ð¼ÐµÑÐ¾Ð² Ð¸Ð· ÑÐ»Ð°Ð¹Ð´ÐµÑÐ¾Ð² 
            local newSize
            if getgenv().HitboxMethod == "Add" then
                local originalSize = args[3].Size or Vector3.new(0, 0, 0)
                newSize = Vector3.new(
                    originalSize.X + getgenv().Size1,
                    originalSize.Y + getgenv().Size2,
                    originalSize.Z + getgenv().Size3
                )
            else
                newSize = Vector3.new(getgenv().Size1, getgenv().Size2, getgenv().Size3)
            end

            -- LEGITIFY (Ð Ð°ÑÑÐ¸ÑÐµÐ½Ð¸Ðµ Ð±ÐµÐ· ÑÐ¼ÐµÑÐµÐ½Ð¸Ñ CFrame)
            if getgenv().HitboxLegitify then
                local modifiedArg3 = {}
                for k, v in pairs(args[3]) do modifiedArg3[k] = v end
                modifiedArg3.Size = newSize
                
                local bestTarget, validTargets = originalHitBox(args[1], args[2], modifiedArg3)
                return bestTarget, validTargets
            else
                -- RAGE Ð ÐÐÐÐ
                return originalHitBox(args[1], args[2], {Size = newSize})
            end
        end
    else
        if originalHitBox then combat.Box = originalHitBox end
    end
end

