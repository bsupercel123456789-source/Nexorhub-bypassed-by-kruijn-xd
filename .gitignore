task.spawn(function()
    local ReplicatedStorage = game:GetService("ReplicatedStorage")
    local Players = game:GetService("Players")
    local LocalPlayer = Players.LocalPlayer

    -- ÐÐ°ÑÑÑÐ¾Ð¹ÐºÐ¸
    getgenv().HitboxMethod = "Override"
    getgenv().Size1 = 15
    getgenv().Size2 = 15
    getgenv().Size3 = 15

    -- ÐÐµÐ·Ð¾Ð¿Ð°ÑÐ½Ð¾Ðµ Ð¿Ð¾Ð´ÐºÐ»ÑÑÐµÐ½Ð¸Ðµ Core (Ð¶Ð´ÐµÐ¼ Ð¼Ð°ÐºÑÐ¸Ð¼ÑÐ¼ 5 ÑÐµÐºÑÐ½Ð´)
    local success, core = pcall(function() 
        return require(ReplicatedStorage:WaitForChild("Core", 5)) 
    end)

    if not success or not core then 
        warn("")
        return 
    end

    -- ÐÐ½ÑÐ¸-ÐÐ°ÑÐ½ÑÐµÑ Ð»Ð¾Ð³Ð¸ÐºÐ°
    local CounteringPlayers = {}
    local CounterAnims = {
        ["rbxassetid://15853335966"] = true,
        ["rbxassetid://17561546657"] = true,
        ["rbxassetid://16024103130"] = true,
    }

    local function Monitor(char, player)
        local hum = char:WaitForChild("Humanoid", 5)
        if hum then
            hum.AnimationPlayed:Connect(function(track)
                if CounterAnims[track.Animation.AnimationId] then
                    CounteringPlayers[player.Name] = true
                    track.Stopped:Connect(function() CounteringPlayers[player.Name] = nil end)
                end
            end)
        end
    end

    for _, v in pairs(Players:GetPlayers()) do
        if v ~= LocalPlayer and v.Character then Monitor(v.Character, v) end
        v.CharacterAdded:Connect(function(c) Monitor(c, v) end)
    end
    Players.PlayerAdded:Connect(function(v) v.CharacterAdded:Connect(function(c) Monitor(c, v) end) end)

    -- Ð¥ÑÐº ÑÐ¸ÑÐ±Ð¾ÐºÑÐ°
    local combat = core.Get("Combat", "Hit")
    local originalHitBox = combat.Box

    combat.Box = function(...)
        if not getgenv().HitboxEnabled then return originalHitBox(...) end
        
        local args = {...}
        local targetChar = originalHitBox(...)
        
        if targetChar then
            local p = Players:GetPlayerFromCharacter(targetChar)
            if p and CounteringPlayers[p.Name] then return nil end
        end

        if args[3] and type(args[3]) == "table" then
            local root = LocalPlayer.Character and LocalPlayer.Character:FindFirstChild("HumanoidRootPart")
            if root then
                -- ÐÐ¸ÑÑÐ°Ð½ÑÐ¸Ð¾Ð½Ð½Ð°Ñ Ð¿ÑÐ¾Ð²ÐµÑÐºÐ° ÐºÐ°ÑÐ½ÑÐµÑÐ°
                for _, p in pairs(Players:GetPlayers()) do
                    if p ~= LocalPlayer and p.Character and p.Character:FindFirstChild("HumanoidRootPart") then
                        if (root.Position - p.Character.HumanoidRootPart.Position).Magnitude <= getgenv().Size3 then
                            if CounteringPlayers[p.Name] then return nil end
                        end
                    end
                end
                -- ÐÑÐ¸Ð¼ÐµÐ½ÐµÐ½Ð¸Ðµ ÑÐ°Ð·Ð¼ÐµÑÐ¾Ð²
                args[3].Size = Vector3.new(getgenv().Size1, getgenv().Size2, getgenv().Size3)
                args[3].CFrame = root.CFrame * CFrame.new(0, 0, -(args[3].Size.Z / 2 - 4))
            end
        end
        return originalHitBox(unpack(args))
    end
end)

