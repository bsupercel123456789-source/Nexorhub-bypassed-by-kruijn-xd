local function createTrail()
    local char = lp.Character or lp.CharacterAdded:Wait()
    local torso = char:WaitForChild("UpperTorso", 5) or char:WaitForChild("Torso", 5)
    if not torso then return end
    if torso:FindFirstChild("CyberTrail") then torso.CyberTrail:Destroy() end
    
    local attTop = Instance.new("Attachment", torso)
    attTop.Name = "AttTop"
    attTop.Position = Vector3.new(0, 1, 0.5)
    local attBottom = Instance.new("Attachment", torso)
    attBottom.Name = "AttBottom"
    attBottom.Position = Vector3.new(0, -1, 0.5)

    local trail = Instance.new("Trail")
    trail.Name = "CyberTrail"
    trail.Parent = torso
    trail.Attachment0 = attTop
    trail.Attachment1 = attBottom
    trail.Texture = "rbxassetid://15542755565"
    trail.Lifetime = 3
    trail.Enabled = trailEnabled
    trail.Color = ColorSequence.new(trailColor)
    trail.Transparency = NumberSequence.new(0, 1)
    trail.WidthScale = NumberSequence.new(1, 0)
    trail.FaceCamera = true
end

local function applyAura()
    local char = lp.Character
    if not char then return end
    local validParts = {["UpperTorso"]=true, ["LowerTorso"]=true, ["Torso"]=true, ["LeftUpperArm"]=true, ["RightUpperArm"]=true, ["LeftUpperLeg"]=true, ["RightUpperLeg"]=true}

    for _, obj in pairs(char:GetChildren()) do
        if obj:IsA("BasePart") and validParts[obj.Name] then
            if obj:FindFirstChild("BodyAura") then obj.BodyAura:Destroy() end
            if not AURA_ENABLED then continue end
            local p = Instance.new("ParticleEmitter")
            p.Name = "BodyAura"; p.Texture = AURA_ID; p.Brightness = 2; p.LightEmission = 0.8; p.ZOffset = 1.5
            p.Color = ColorSequence.new(AURA_COLOR)
            p.Transparency = NumberSequence.new({NumberSequenceKeypoint.new(0, 1), NumberSequenceKeypoint.new(0.5, 0.7), NumberSequenceKeypoint.new(1, 1)})
            p.Size = NumberSequence.new(2); p.LockedToPart = true; p.Lifetime = NumberRange.new(0.8); p.Rate = 10; p.Speed = NumberRange.new(0)
            p.Parent = obj
        end
    end
end

