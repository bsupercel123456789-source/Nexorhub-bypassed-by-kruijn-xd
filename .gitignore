local function applyLighting(Mode)
    _G.CycleActive = false
    local base = _G.OriginalSettings
    if Mode == "None" then
        lighting.Brightness = base.Brightness
        lighting.OutdoorAmbient = base.OutdoorAmbient
        lighting.ClockTime = base.ClockTime
    elseif Mode == "Sun" then
        lighting.ClockTime = 11.5
        lighting.Brightness = 2
    elseif Mode == "Night" then
        lighting.ClockTime = 0
        lighting.Brightness = 10 
        lighting.OutdoorAmbient = Color3.fromRGB(35, 35, 55)
    elseif Mode == "Cycle" then
