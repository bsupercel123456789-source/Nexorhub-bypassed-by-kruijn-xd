local function lag_machine_combo()
    local ReplicatedStorage = game:GetService("ReplicatedStorage")
    local lp = game:GetService("Players").LocalPlayer
    
    local function getTarget()
        local players = game:GetService("Players"):GetPlayers()
        local minDist = 50 
        for _, p in pairs(players) do
            if p ~= lp and p.Character and p.Character:FindFirstChild("HumanoidRootPart") then
                local dist = (lp.Character.HumanoidRootPart.Position - p.Character.HumanoidRootPart.Position).Magnitude
                if dist < minDist then
                    return p.Character
                end
            end
        end
        return nil
    end

    local char = lp.Character
    local charData = lp:FindFirstChild("Data")
    local charValue = charData and charData:FindFirstChild("Character") and charData.Character.Value
    local target = getTarget()
    
    if char and charValue and target then
        local wallComboAbility = ReplicatedStorage.Characters[charValue]:FindFirstChild("WallCombo")
        if wallComboAbility then
            for i = 1, 50 do
                local actionNumber = "Action" .. math.random(1000, 9999)
                local randomId = math.random(100000, 999999)
                local randomOffset = Vector3.new(math.random(-5, 5), math.random(-5, 5), math.random(-5, 5))
                local jitterCFrame = target.HumanoidRootPart.CFrame * CFrame.new(randomOffset)
                
                local remoteArgs = {
                    wallComboAbility,
                    "Characters:" .. charValue .. ":WallCombo",
                    1, 
                    randomId,
                    {
                        HitboxCFrames = {jitterCFrame},
                        BestHitCharacter = target,
                        HitCharacters = {target},
                        Ignore = {[actionNumber] = {target}},
                        DeathInfo = {},
                        Actions = {[actionNumber] = {}},
                        HitInfo = { Blocked = false, IsFacing = true, IsInFront = true },
                        BlockedCharacters = {},
                        ServerTime = tick(),
                        FromCFrame = char.HumanoidRootPart.CFrame
                    },
                    actionNumber
                }
                
                pcall(function() 
                    ReplicatedStorage.Remotes.Abilities.Ability:FireServer(wallComboAbility, randomId)
                    ReplicatedStorage.Remotes.Combat.Action:FireServer(unpack(remoteArgs))
                end)
            end

            task.spawn(function()
                if char:FindFirstChild("Action") then char.Action:Destroy() end
                if char:FindFirstChild("Busy") then char.Busy:Destroy() end
                local hum = char:FindFirstChildOfClass("Humanoid")
                if hum then
                    for _, anim in pairs(hum:GetPlayingAnimationTracks()) do
                        if anim.Name:find("WallCombo") then anim:Stop() end
                    end
                end
            end)
        end
    end
end

